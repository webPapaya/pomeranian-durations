{
  "name": "pomeranian-durations",
  "version": "1.2.0",
  "description": "An immutable duration library based on the ISO-8601 format for durations.",
  "main": "lib/index.js",
  "scripts": {
    "docs": "documentation build src/** -f md > doc.md && git commit -m 'Update docs' doc.md || true",
    "build": "node src/_generate_index.js && babel src/ --out-dir lib/ --ignore *.spec.js",
    "lint": "eslint-crewmeister src",
    "test": "mocha src/{*,**/*,**/**/*,**/**/**/*}.spec.js --recursive --compilers js:babel-register",
    "preversion": "npm run lint && npm run test && npm run docs && npm run build",
    "prepush": "npm run docs"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/webPapaya/pomeranian.git"
  },
  "devDependencies": {
    "babel-cli": "^6.10.1",
    "babel-core": "^6.9.1",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-0": "^6.5.0",
    "babel-register": "^6.9.0",
    "documentation": "^5.4.0",
    "eslint-config-crewmeister": "latest",
    "hamjest": "^2.13.0",
    "husky": "^0.14.3",
    "mocha": "^2.5.3"
  },
  "keywords": [
    "time",
    "duration",
    "ISO-8601",
    "ISO",
    "8601"
  ],
  "author": "Webpapaya",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/webPapaya/pomeranian/issues"
  },
  "homepage": "https://github.com/webPapaya/pomeranian#readme"
}
